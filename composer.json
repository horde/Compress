    "autoload": {
        "psr-0": {
            "Horde_Compress": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Horde\\Compress\\Test\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "horde/horde-installer-plugin": true
        }
    }
}