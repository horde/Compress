    "suggest": {
        "horde/icalendar": "^3 || dev-FRAMEWORK_6_0",
        "horde/mail": "^3 || dev-FRAMEWORK_6_0",
        "horde/mapi": "^2 || dev-FRAMEWORK_6_0",
        "horde/stream_filter": "^3 || dev-FRAMEWORK_6_0",
        "ext-zlib": "*"
